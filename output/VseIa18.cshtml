            searchUrl: "@Url.Action("Search", "TelSearch")",
            beforeApplyData: function (result) {
                if(result['tel:street']){
                    result['tel:street'] += ' ' + result['tel:streetno'];
                }
                if (result['tel:firstname']) {
                    result['tel:firstname'] += ' ' + result['tel:name'];
                } else {
                    result['tel:firstname'] = result['tel:name'];
                }
