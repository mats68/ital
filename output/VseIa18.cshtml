            $("#ANSCHASVOR").hide();
            $("#ANSCHMOD").hide();
            $("#ANSCHHAKINT").hide();
            $("#ANSCHHAKNR").hide();
            $("#ANSCHNEU").hide();
            $("#ANSCHVER").hide();
            $("#ANSCHWECHS").hide();
            $("#ANSCHASERF").show();
        }
        else if ($("#optionsHAGKBestehend").prop('checked'))
        {
            $("#optHAGKNeu").show();
            $("#optHAGKBestehend").show();
            $("#ANSCHKAB").show();
            $("#ANSCHFREI").show();
            $("#ANSCHORT").show();
            $("#ANSCHMAXBEZ").show();
            $("#ANSCHMAXSPEIS").show();
            $("#ANSCHASVOR").show();
            $("#ANSCHMOD").show();
            $("#ANSCHHAKINT").show();
            $("#ANSCHHAKNR").hide();
            $("#ANSCHNEU").show();
            $("#ANSCHVER").show();
            $("#ANSCHWECHS").show();
            $("#ANSCHASERF").show();
            toggleAnschlussNeu();
        }
        else
        {
            $("#optHAGKNeu").show();
            $("#optHAGKBestehend").show();
            $("#ANSCHKAB").show();
            $("#ANSCHFREI").show();
            $("#ANSCHORT").show();
            $("#ANSCHMAXBEZ").hide();
            $("#ANSCHMAXSPEIS").hide();
            $("#ANSCHASVOR").hide();
            $("#ANSCHMOD").hide();
            $("#ANSCHHAKINT").hide();
            $("#ANSCHHAKNR").hide();
            $("#ANSCHNEU").hide();
            $("#ANSCHVER").hide();
            $("#ANSCHWECHS").hide();
            $("#ANSCHASERF").hide();
        }
    }

    function toggleAnschlussNeu()
    {
        if ($("#optionsANSCH_NEU").prop('checked'))
        {
            $("#optHAGKNeu").show();
            $("#optHAGKBestehend").show();
            $("#ANSCHKAB").show();
            $("#ANSCHFREI").show();
            $("#ANSCHORT").show();
            $("#ANSCHMAXBEZ").hide();
            $("#ANSCHMAXSPEIS").hide();
            $("#ANSCHASVOR").show();
            $("#ANSCHMOD").show();
            $("#ANSCHHAKINT").show();
            $("#ANSCHHAKNR").hide();
            $("#ANSCHNEU").show();
            $("#ANSCHVER").show();
            $("#ANSCHWECHS").show();
            $("#ANSCHASERF").hide();
        }
        else if ($("#optionsANSCH_VER").prop('checked') || $("#optionsANSCH_WECHS").prop('checked')) {
