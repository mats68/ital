        colmap["tAListe.RECHADR"] = 1;
        colmap["tAListe.GEBTEIL"] = 2;
        colmap["tAListe.NUTZUNG"] = 3;
        colmap["tAListe.STOCKWERK"] = 4;
        colmap["tAListe.EVUANLNR"] = 5;
        colmap["tAListe.RAUM"] = 6;
        colmap["tAListe.TARIF"] = 7;
        colmap["tAListe.SICH_IN"] = 8;
        colmap["tAListe.WERKNR"] = 9;
        colmap["tAListe.MONT_ORT"] = 10;
        colmap["tAListe.ZEV"] = 11;
        colmap["tAListe.ANZ_AUSSENL"] = 12;
        colmap["tAListe.NEU"] = 13;
        colmap["tAListe.VORH"] = 14;
        colmap["tAListe.AUSW"] = 15;
        colmap["tAListe.DEMONT"] = 16;
        colmap["tAListe.UMMONT"] = 17;


        colmap["vGListe.ANZAHL"] = 0;
        colmap["vGListe.VERBRAUCHER"] = 1;
        colmap["vGListe.ERZEUGER"] = 2;
        colmap["vGListe.SPEICHER"] = 3;
        colmap["vGListe.TEXT"] = 4;
        colmap["vGListe.GES_DATUM"] = 5;
        colmap["vGListe.KVA"] = 6;
        colmap["vGListe.KVA_ANS_NETZ"] = 7;
        colmap["vGListe.SPEICH_KAP"] = 8;
        colmap["vGListe.ANZ_AUSSENL"] = 9;

        // Änderungen von VNB anzeigen
        m2web.form.showDifferences("@Url.Action("GetVnbDifferences")/" + @Model.DocumentId, colmap);

        var onText = '@("switchYes".Translate(documentCultureInfo))';
        var offText = '@("switchNo".Translate(documentCultureInfo))';
        m2web.formIa18.init(@Model.DocumentId, formAddresses, '@Culture', onText, offText);

        m2web.form.setInfoTooltips([{id: 'InfoForSwitchMessSteuerList', title: '@(trans.GetText("Wenn mehr als ein Eintrag in der Liste vorhanden ist, bleibt der Schalter eingeschalten und deaktiviert", "Si plus d'une entrée dans la liste est présent, le commutateur reste allumé et éteint","Se nell'elenco è presente più di una voce, l'interruttore rimane acceso e disattivato"))'},
                                    {id: 'InfoForSwitchVerbrauList', title: '@(trans.GetText("Wenn die Liste nicht leer ist, bleibt der Schalter eingeschalten und deaktiviert", "Si la liste ne soit pas vide, le commutateur reste allumé et éteint","Se l'elenco non è vuoto, l'interruttore rimane acceso e disattivato"))'}]);

        $('#div_address').telsearch({
            firstname: "GES_NAME1",
            street: "GES_ADR1",
            streetno: "AnlageAddress_txbAddresseNr",
            zip: "GES_PLZ",
            city: "GES_ORT",
            phone: "GES_TELNR",
