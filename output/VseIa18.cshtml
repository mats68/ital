                                </label>
                            </div>
                            <div class="col-xs-3 radio radio-primary" id="optSpracheFr">
                                <label>
                                    @Html.RadioButton("oSprache", 1, "f".CompareTo(Model.U_SPRACHE) == 0, new { @name = "optionsSprache", @id = "optionsSpracheFr" })
                                    <span class="circle"></span><span class="check"></span>
                                    <span>Fr</span>
