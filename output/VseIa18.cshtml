                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.U_NAME1)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.U_NAME2)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.U_ADRESSE1)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.U_PLZ)
                        </div>
                        <div class="col-xs-12">
