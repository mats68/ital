                        <div class="col-xs-3">
                            @Html.TextBoxFor(a => a.ABONR, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6 text-right">
                            @Html.ValidationMessageFor(a => a.ABONR, null, new { @class = "text-danger" })
                        </div>
                    </div>
                    <div class="row">
                        @Html.M2WebFormLabelFor2(a => a.ZAEHLER, trans.GetText("Zähler-Nr.", "Compteur n°"), new { @class = "col-xs-3 control-label labelPadding" })
                        <div class="col-xs-8 col-md-6">
                            @Html.M2WebDisplayTextFor(a => a.ZAEHLER)
                        </div>
                    </div>
                </div>
                <!-- #endregion -->
