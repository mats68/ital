                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_NAME2)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_ADRESSE1)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_ADRESSE2)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_PLZ)
