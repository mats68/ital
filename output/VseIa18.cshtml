                    <div class="col-xs-6">
                        <div class="row">
                            <div class=" col-xs-4 checkbox">
                                <label>
                                    @Html.CheckBoxFor(a => a.I_NEW, new { @class = "inst-beschrieb", onchange = "toggleNetzanschluss()" })<span class="check"></span>
