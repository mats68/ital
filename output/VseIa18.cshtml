                                </div>
                            </div>
                            <div class="form-group">
                                @Html.M2WebFormLabelFor2(a => a.GES_TELNR, trans.GetText("Telefon", "Téléphone","Telefono"), new { @class = "col-xs-4 col-sm-4 control-label" })
