                            @Html.M2WebDisplayTextFor(a => a.U_TELNR)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebFormLabelFor2(a => a.U_SPRACHE, trans.GetText("Sprache", "Langue","Linguaggio"), new { @class = "control-label left" })
