                            <h4 class="col-xs-12 small">@(trans.GetText("Sachbearbeiter", "Responsable","Impiegato"))</h4>
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_SACHB)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_EMAIL)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_TELNR)
                        </div>
                        <div class="col-xs-12">
                            @Html.M2WebDisplayTextFor(a => a.I_FAXNR)
